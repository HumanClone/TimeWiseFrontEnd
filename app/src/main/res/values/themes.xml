<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TimeWiseFrontEnd" parent="Theme.Material3.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#088395</item>
        <item name="colorPrimaryVariant">#05BFDB</item>
        <item name="colorOnPrimary">#AFD3E2</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#E60965</item>
        <item name="colorSecondaryVariant">#F94892</item>
        <item name="colorOnSecondary">#FFA1C9</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#6C00FF</item>
        <!-- Customize your theme here. -->
        <item name="backgroundColor">#E7E7E7</item>

    </style>

    <style name="Theme.TimeWiseFrontEnd.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="Theme.TimeWiseFrontEnd.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TimeWiseFrontEnd.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>